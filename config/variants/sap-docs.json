{
  "id": "sap-docs",
  "description": "Full SAP documentation profile",
  "sourceAllowlist": [
    "/sapui5",
    "/cap",
    "/openui5-api",
    "/openui5-samples",
    "/wdi5",
    "/ui5-tooling",
    "/cloud-mta-build-tool",
    "/ui5-webcomponents",
    "/cloud-sdk-js",
    "/cloud-sdk-java",
    "/cloud-sdk-ai-js",
    "/cloud-sdk-ai-java",
    "/ui5-typescript",
    "/ui5-cc-spreadsheetimporter",
    "/abap-cheat-sheets",
    "/sap-styleguides",
    "/dsag-abap-leitfaden",
    "/abap-fiori-showcase",
    "/cap-fiori-showcase",
    "/abap-platform-rap-opensap",
    "/cloud-abap-rap",
    "/abap-platform-reuse-services",
    "/abap-docs-standard",
    "/abap-docs-cloud"
  ],
  "submodulePaths": [
    "sources/sapui5-docs",
    "sources/cap-docs",
    "sources/openui5",
    "sources/wdi5",
    "sources/ui5-tooling",
    "sources/cloud-mta-build-tool",
    "sources/ui5-webcomponents",
    "sources/cloud-sdk",
    "sources/cloud-sdk-ai",
    "sources/ui5-typescript",
    "sources/ui5-cc-spreadsheetimporter",
    "sources/abap-cheat-sheets",
    "sources/sap-styleguides",
    "sources/dsag-abap-leitfaden",
    "sources/abap-fiori-showcase",
    "sources/cap-fiori-showcase",
    "sources/abap-platform-rap-opensap",
    "sources/cloud-abap-rap",
    "sources/abap-platform-reuse-services",
    "sources/abap-docs"
  ],
  "tools": {
    "abapLint": false,
    "abapFeatureMatrix": true
  },
  "server": {
    "stdioName": "Local SAP Docs",
    "stdioDescription": "Offline SAPUI5 & CAP documentation server with SAP Community, SAP Help Portal, and ABAP Keyword Documentation integration",
    "streamableName": "SAP Docs Streamable HTTP",
    "streamableDescription": "SAP documentation server with Streamable HTTP transport - supports SAPUI5, CAP, wdi5, SAP Community, SAP Help Portal, and ABAP Keyword Documentation integration",
    "httpStatusPort": 3001,
    "streamablePort": 3122,
    "pm2HttpName": "mcp-sap-http",
    "pm2StreamableName": "mcp-sap-streamable",
    "deployPath": "/opt/mcp-sap/mcp-sap-docs"
  },
  "metadataPath": "src/metadata.json"
}
